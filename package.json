{
  "name": "vue3-highlight-for-start-to-end",
  "version": "1.0.0",
  "description": "vue3根据文字起始位置和结束位置高亮",
  "author": "三火Sunfire",
  "license": "MIT",
  "files": ["dist"],
  "main": "./dist/highlight.es.ts",
  "module": "./dist/highlight.umd.ts",
  "exports": {
    ".": {
      "import": "./dist/highlight.es.ts",
      "require": "./dist/highlight.umd.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a1419735487/vue3-highlight-for-start-to-end"
  },
  "scripts": {
    "serve": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.2.0",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vue-tsc": "^0.29.8"
  }
}
